{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolvePackageJsonImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  }
}